<!doctype html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport"
    content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Document</title>
  <link rel="stylesheet" href="/css/style.css">
  <script src="/js/page.js" defer></script>
  <!-- include the Bootstrap CSS file -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css">

  <!-- include the jQuery library (required for Summernote) -->
  <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>

  <!-- include the Bootstrap JavaScript bundle (optional) -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"></script>

  <!-- include the Summernote CSS file -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.css">

  <!-- include the Summernote JavaScript file -->
  <script src="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.js"></script>

</head>


<body>
  <div class="container mt-4">


    <div id="ticketdataupload">
    </div>

    <div id="ticketContainer">
    </div>

    <div class="container mt-4" id="Containesubmit">
      <!-- Submit a Ticket Modal -->
      <form id="submitMessageForm">
        <div class="form-group">
          <div id="summernote"></div>
        </div>
        <button type="submit" id="submitMessageButton" class="btn btn-primary">Submit</button>
        <button type="button" class="btn btn-primary float-right" data-toggle="modal"
          data-target="#closeModal">Close</button>
      </form>
    </div>
  </div>



  </div>


  <!-- Close Modal -->
  <div class="modal fade" id="closeModal" tabindex="-1" role="dialog" aria-labelledby="closeModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="closeModalLabel">Confirm Close</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          Are you sure you want to close this topic?
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
          <button type="button" class="btn btn-primary" id="confirmClose">Close</button>
        </div>
      </div>
    </div>
  </div>

</body>

</html>