<div class="container mt-4" id="Containesubmit">
    <!-- Submit a Ticket Modal -->
    <form>
        <div class="form-group">
            <label for="ticketTitle" class="col-form-label">Title:</label>
            <input type="text" class="form-control" id="ticketTitle">
        </div>
        <div class="form-group">
            <label for="status">Тип проблемы:</label>
            <select class="form-control" id="status">
                <option>Проблема 1</option>
                <option>Проблема 2</option>
                <option>Проблема 3</option>
            </select>
        </div>
        <div class="form-group">
            <div id="summernote"></div>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>